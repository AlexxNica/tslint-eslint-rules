{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "test",
      "type": "node",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "args": [
        "dist/test/**/*.js"
      ],
      "cwd": "${workspaceRoot}/.",
      "runtimeExecutable": null,
      "preLaunchTask": "build",
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "outDir": "${workspaceRoot}/dist",
      "request": "launch"
    }
  ]
}
